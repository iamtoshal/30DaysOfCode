//Initial Template for C++


#include<bits/stdc++.h>
using namespace std;



// } Driver Code Ends
//User function Template for C++
class Solution
{
    public:
    string reverseWord(string str)
    {
        // Your code goes here
        int n = str.length();
        int start=0;
        
        for(int i=0;i<n/2;i++){
            char temp = str[i];
            str[i]=str[n-i-1];
            str[n-i-1]=temp;
        }
        return str;
    }
    
};



//{ Driver Code Starts.

int main() {
	
	int t;
	cin>>t;
	while(t--)
	{
    	string s;
    	cin >> s;
    	Solution ob;
    	cout << ob.reverseWord(s) << endl;
	}
	return 0;
	
}


// } Driver Code Ends
